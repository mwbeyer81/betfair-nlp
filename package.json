{
  "name": "betfair-nlp",
  "version": "1.0.0",
  "description": "Betfair historical data processing with MongoDB",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "server": "ts-node src/server/index.ts",
    "server:dev": "ts-node-dev --respawn src/server/index.ts",
    "server:docker": "docker-compose -f docker-compose.local.yml up -d",
    "server:docker:down": "docker-compose -f docker-compose.local.yml down",
    "server:docker:logs": "docker-compose -f docker-compose.local.yml logs -f api-server",
    "server:docker:build": "docker-compose -f docker-compose.local.yml build --no-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:service": "jest src/lib/service",
    "test:server": "jest src/server",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "format:all": "prettier --write .",
    "process:file": "ts-node src/commands/process-file.ts",
    "process:directory": "ts-node src/commands/process-directory.ts",
    "process:event": "ts-node src/commands/process-event.ts",
    "process:basic:docker-mongo": "ts-node src/commands/process-basic-files.ts",
    "process:basic:docker": "docker-compose -f docker-compose.local.yml exec api-server yarn process:basic:local",
    "uncompress:bz2": "ts-node src/commands/uncompress-bz2.ts",
    "mongo:drop-all": "docker-compose -f docker-compose.local.yml exec mongodb mongosh betfair_nlp_dev --eval \"$(cat scripts/drop-all-collections.js)\"",
    "config:show": "node scripts/show-config.js"
  },
  "keywords": [
    "betfair",
    "mongodb",
    "typescript",
    "nlp"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "config": "^4.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.1",
    "openai": "^5.16.0"
  },
  "devDependencies": {
    "@types/config": "^3.3.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  }
}