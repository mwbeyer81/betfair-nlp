{
  "name": "betfair-nlp",
  "version": "1.0.0",
  "description": "Betfair historical data processing with MongoDB",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:service": "jest src/lib/service",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "format:all": "prettier --write .",
    "process:file": "ts-node src/commands/process-file.ts",
    "process:directory": "ts-node src/commands/process-directory.ts",
    "process:event": "ts-node src/commands/process-event.ts"
  },
  "keywords": [
    "betfair",
    "mongodb",
    "typescript",
    "nlp"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "config": "^4.1.1",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "@types/config": "^3.3.3",
    "@types/jest": "^29.5.8",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}